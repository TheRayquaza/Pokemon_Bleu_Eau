[application]

config/name="Pokemon Bleu Eau"
config/description="A Pokemon version like of Red Fire and Green Leaf"
run/main_scene="res://GUI/StartMenu/Menu.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://img Pokemon/All Pokemon/Front/9.png"
config/windows_native_icon="res://img Pokemon/9.ico"

[autoload]

Pokemon="*res://Pokemon.gd"
PG="*res://PlayerGlobale.gd"
EG="*res://EnnemiGlobale.gd"
PSS="*res://PokemonStorageSystem.gd"
Save="*res://Others/Save/SaveGame.gd"
Attaque="*res://Attaque.gd"
UISpeack="*res://UISpeackGlobale.gd"
